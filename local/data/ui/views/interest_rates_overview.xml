<form theme="dark" script="custom_layout_width.js" stylesheet="auto_resize_input.css">
  <label>Interest Rates</label>
  <search>
    <query>
              <![CDATA[index="fred_data" SeriesID=DGS2 | sort Date desc | fields Date Value | eval Value=tostring(Value, "commas") | head 1 | table Date Value]]>
            </query>
    <done>
      <condition match=" 'job.resultCount' &gt; 0">
        <set token="DGS2_DT_TOK">$result.Date$</set>
      </condition>
      <condition>
        <set token="DGS2_DT_TOK">"No Data"</set>
      </condition>
    </done>
    <earliest>-7d</earliest>
    <latest>now</latest>
    <sampleRatio>1</sampleRatio>
  </search>
