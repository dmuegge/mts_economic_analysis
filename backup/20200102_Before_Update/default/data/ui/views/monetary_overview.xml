<form script="custom_layout_width.js" stylesheet="auto_resize_input.css" theme="dark">
  <label>Monetary</label>
  <search id="all_monetary_ttm_base">
    <query>
      `mts_econ_indexes` SeriesID IN ([| inputlookup FRED_Default_List where (Category="Monetary Data") | fields FredID | mvcombine FredID | eval fid=mvjoin(FredID,",") | return $fid]) Value!="."
| stats last(Value) as Value by _time SeriesID
    </query>
    <earliest>-12mon</earliest>
    <latest>now</latest>
  </search>
  <search>
    <query>
      <![CDATA[`mts_econ_indexes` SeriesID IN ([| inputlookup FRED_Default_List where (Category="Monetary Data") | fields FredID | mvcombine FredID | eval fid=mvjoin(FredID,",") | return $fid]) | sort Date desc | dedup SeriesID | table SeriesID Date | transpose 0 column_name=SeriesID header_field=SeriesID  | fields - SeriesID]]>
    </query>
    <done>
      <condition match=" 'job.resultCount' &gt; 0">
        <set token="M1_DT_TOK">$result.M1$</set>
        <set token="M2_DT_TOK">$result.M2$</set>
        <set token="RPONTTLD_DT_TOK">$result.RPONTTLD$</set>
        <set token="RRPONTTLD_DT_TOK">$result.RRPONTTLD$</set>
        <set token="MBCURRCIRW_DT_TOK">$result.MBCURRCIRW$</set>
        <set token="RESBALNSW_DT_TOK">$result.RESBALNSW$</set>
      </condition>
      <condition>
        <set token="M1_DT_TOK">"No Data"</set>
        <set token="M2_DT_TOK">"No Data"</set>
        <set token="RPONTTLD_DT_TOK">"No Data"</set>
        <set token="RRPONTTLD_DT_TOK">"No Data"</set>
        <set token="MBCURRCIRW_DT_TOK">"No Data"</set>
        <set token="RESBALNSW_DT_TOK">"No Data"</set>
      </condition>
    </done>
    <earliest>-30d</earliest>
    <latest>now</latest>
    <sampleRatio>1</sampleRatio>
  </search>
  
  <fieldset submitButton="false">
    <input type="time" token="time_tok" searchWhenChanged="true">
      <label>Time</label>
      <default>
        <earliest>-5y</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
