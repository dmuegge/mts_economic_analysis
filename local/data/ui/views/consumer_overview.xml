<form script="custom_layout_width.js" stylesheet="auto_resize_input.css" theme="dark">
  <label>Consumer</label>
  <search id="all_consumer_ttm_base">
    <query>
      index="fred_data" SeriesID IN ([| inputlookup FRED_Default_List where (Category="Consumer") | fields FredID | mvcombine FredID | eval fid=mvjoin(FredID,",") | return $fid]) Value!="."
| stats last(Value) as Value by _time SeriesID
    </query>
    <earliest>-12mon</earliest>
    <latest>now</latest>
  </search>
  <search>
    <query>
      <![CDATA[index="fred_data" SeriesID IN ([| inputlookup FRED_Default_List where (Category="Consumer") | fields FredID | mvcombine FredID | eval fid=mvjoin(FredID,",") | return $fid]) | sort Date desc | dedup SeriesID | table SeriesID Date | transpose 0 column_name=SeriesID header_field=SeriesID  | fields - SeriesID]]>
    </query>
    <done>
      <condition match=" 'job.resultCount' &gt; 0">
        <set token="CPIAUCSL_DT_TOK">$result.CPIAUCSL$</set>
        <set token="PCE_DT_TOK">$result.PCE$</set>
        <set token="PI_DT_TOK">$result.PI$</set>
        <set token="PSAVERT_DT_TOK">$result.PSAVERT$</set>
      </condition>
      <condition>
        <set token="CPIAUCSL_DT_TOK">"No Data"</set>
        <set token="PCE_DT_TOK">"No Data"</set>
        <set token="PI_DT_TOK">"No Data"</set>
        <set token="PSAVERT_DT_TOK">"No Data"</set>
      </condition>
    </done>
    <earliest>-1y</earliest>
    <latest>now</latest>
    <sampleRatio>1</sampleRatio>
  </search>
  <fieldset submitButton="false">
    <input type="time" token="time_tok" searchWhenChanged="true">
      <label>Time</label>
      <default>
        <earliest>-5y</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
